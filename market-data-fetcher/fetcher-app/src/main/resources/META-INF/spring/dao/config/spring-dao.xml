	<bean id="stockDao" class="com.finaxys.finaxysplatform.marketdatafetcher.dao.impl.StockDaoImpl" />


	<bean id="optionChainDao"
		class="com.finaxys.finaxysplatform.marketdatafetcher.dao.impl.OptionChainDaoImpl" />
		
		
	<bean id="optionDao"
		class="com.finaxys.finaxysplatform.marketdatafetcher.dao.impl.OptionDaoImpl" />


